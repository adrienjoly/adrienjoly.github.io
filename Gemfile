# Run `bundle install` to install Jekyll, for static md to html file generation.
# see https://docs.github.com/en/pages/setting-up-a-github-pages-site-with-jekyll/testing-your-github-pages-site-locally-with-jekyll
source 'https://rubygems.org'
ruby File.read(".ruby-version").strip
gem 'dotenv'
gem "github-pages", "~> 231", group: :jekyll_plugins
gem 'jekyll-seo-tag', group: :jekyll_plugins
gem "webrick", "~> 1.3" # cf https://github.com/imagej/imagej.github.io/pull/133
